{
  "include": ["./*"],
  "exclude": ["./node_modules/"],
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "CommonJS",                                
    "rootDir": "./",                                  
    "esModuleInterop": true,                                      
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                 
  }, 
}